{
  "name": "nifty-react",
  "version": "0.0.1",
  "description": "基于vite、react、antd的脚手架工具",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "ZHT",
  "keywords": [
    "nifty",
    "nifty-react",
    "react",
    "antd",
    "vite"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "main": "./dist/nifty-react.umd.cjs",
  "module": "./dist/nifty-react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nifty-react.js",
      "require": "./dist/nifty-react.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "antd": "^5.18.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "rollup-plugin-copy": "^3.5.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "peerDependencies": {
    "antd": ">=5.18.3",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
